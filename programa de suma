#include <iostream>

using namespace std;

int main(int argc, char** argv) {
	
	int a= 0;
	int b= 0;
	int resultado= 0;
	
	cout<<"ingrese el valor de a:";
	cin>>a;
	cout<<endl;
	
	cout<<"ingrese el valor de b:";
	cin>>b;
	resultado=a + b;
	
	cout<<endl;
	cout<<"la suma de a+b es:"<<resultado;
	
	return 0;
}
